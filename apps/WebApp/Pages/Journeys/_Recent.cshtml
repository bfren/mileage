@using Mileage.Domain.GetRecentJourneys
@model Mileage.WebApp.Pages.Journeys.RecentModel
@{
	ArgumentNullException.ThrowIfNull(Model);

	var listModel = new ListModel("Recent", "recent-journeys") { Journeys = Model.Journeys };
}

<div id="recent-journeys">
	<partial name="_List" model="@listModel"/>
</div>
