{
  "jeebs": {
    "app": {
      "name": "Web App",
      "suite": "Mileage"
    },
    "db": {
      "default": "rpi-docker",
      "authentication": "rpi-docker",
      "connections": {
        "rpi-docker": {
          "connectionString": ""
        }
      }
    },
    "logging": {
      "minimum": "Verbose",
      "providers": {
        "console.serilog": {
          "enabled": true
        }
      }
    },
    "services": {
      "console": {
        "serilog": {
          "addPrefix": false
        }
      }
    },
    "web": {
      "auth": {
        "enabled": true,
        "scheme": "jwt"
      }
    }
  }
}