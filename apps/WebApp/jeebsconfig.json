{
  "jeebs": {
    "app": {
      "name": "Web App",
      "suite": "Mileage"
    },
    "db": {
      "default": "rpi",
      "authentication": "rpi",
      "connections": {
        "rpi": {
          "connectionString": ""
        }
      }
    },
    "logging": {
      "minimum": "Verbose",
      "providers": {
        "console.serilog": {
          "enabled": true
        }
      }
    },
    "services": {
      "console": {
        "serilog": {
          "addPrefix": false
        }
      }
    },
    "web": {
      "auth": {
        "enabled": true,
        "scheme": "cookies"
      }
    }
  }
}